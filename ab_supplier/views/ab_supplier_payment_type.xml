<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ab_supplier_payment_type_view_tree" model="ir.ui.view">
            <field name="name">ab_supplier_payment_type_view_tree</field>
            <field name="model">ab_supplier_payment_type</field>
            <field name="arch" type="xml">
                <tree string="ab_supplier_payment_type_tree" multi_edit="1">
                    <field name="name" optional="show"/>
                </tree>
            </field>
        </record>

        <record id="ab_supplier_payment_type_view_form" model="ir.ui.view">
            <field name="name">ab_supplier_payment_type_view_form</field>
            <field name="model">ab_supplier_payment_type</field>
            <field name="arch" type="xml">
                <form string="ab_supplier_payment_type_form">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="ab_supplier_payment_type_view_search" model="ir.ui.view">
            <field name="name">ab_supplier_payment_type_view_search</field>
            <field name="model">ab_supplier_payment_type</field>
            <field name="arch" type="xml">
                <search string="ab_supplier_payment_type_search">
                    <field name="name"/>
                </search>
            </field>
        </record>
        <record id="ab_supplier_payment_type_view_pivot" model="ir.ui.view">
            <field name="name">ab_supplier_payment_type_view_pivot</field>
            <field name="model">ab_supplier_payment_type</field>
            <field name="arch" type="xml">
                <pivot string="ab_supplier_payment_type_pivot">
                </pivot>
            </field>
        </record>


        <record id="ab_supplier_payment_type_action" model="ir.actions.act_window">
            <field name="name">Ab supplier payment type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ab_supplier_payment_type</field>
            <field name="view_mode">tree,form,pivot</field>
        </record>


    </data>
</odoo>